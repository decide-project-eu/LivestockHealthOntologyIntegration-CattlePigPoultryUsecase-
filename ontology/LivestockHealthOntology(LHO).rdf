        <rdfs:subClassOf rdf:resource="http://www.purl.org/decide#Pathogen"/>
        <rdfs:comment xml:lang="en">Mycoplasma bovis is a bacterial pathogen that primarily affects cattle, causing respiratory disease, arthritis, mastitis, and other systemic infections.</rdfs:comment>
        <rdfs:isDefinedBy>https://agrovoc.fao.org/browse/agrovoc/en/page/c_27241</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">MycoplasmaBovis</rdfs:label>
        <hasExactSynonym>MB</hasExactSynonym>
    </owl:Class>
    


    <!-- http://aims.fao.org/aos/agrovoc/c_27245 -->

