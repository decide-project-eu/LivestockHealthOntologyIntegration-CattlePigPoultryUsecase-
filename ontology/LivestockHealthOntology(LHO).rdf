    <!-- http://aims.fao.org/aos/agrovoc/c_16398 -->

    <owl:Class rdf:about="http://aims.fao.org/aos/agrovoc/c_16398">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/decide#Pathogen"/>
        <rdfs:comment xml:lang="en">Bovine Respiratory Syncytial Virus (BRSV) is a major cause of respiratory disease in cattle, characterized by coughing, nasal discharge, and increased respiratory rate.</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://agrovoc.fao.org/browse/agrovoc/en/page/c_16398</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">BovineRespiratorySyncytialVirus</rdfs:label>
        <hasExactSynonym xml:lang="en">BRSV</hasExactSynonym>
    </owl:Class>
    


    <!-- http://aims.fao.org/aos/agrovoc/c_27241 -->

