        <rdfs:subClassOf rdf:resource="http://www.purl.org/decide#Pathogen"/>
        <rdfs:comment xml:lang="en">Pasteurella haemolytica is a Gram-negative bacterium associated with respiratory infections, particularly in cattle, causing bovine respiratory disease complex (BRDC) or shipping fever.</rdfs:comment>
        <rdfs:isDefinedBy>https://agrovoc.fao.org/browse/agrovoc/en/page/c_34495</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">PasturellaHaemolytica</rdfs:label>
        <hasExactSynonym>PH</hasExactSynonym>
    </owl:Class>
    


    <!-- http://aims.fao.org/aos/agrovoc/c_35380 -->

