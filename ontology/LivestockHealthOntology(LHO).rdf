        <rdfs:subClassOf rdf:resource="http://www.purl.org/decide#Pathogen"/>
        <rdfs:comment xml:lang="en">Mycoplasma hyopneumoniae is a bacterium causing swine enzootic pneumonia, a respiratory disease in pigs, characterized by coughing and impaired respiratory function.</rdfs:comment>
        <rdfs:label xml:lang="en">MycoplasmaHyopneumoniae</rdfs:label>
        <hasExactSynonym>M.Hyopneumoniae</hasExactSynonym>
        <hasExactSynonym xml:lang="en">M_Hyopneumoniae</hasExactSynonym>
    </owl:Class>
    


    <!-- http://aims.fao.org/aos/agrovoc/c_33359 -->

    <owl:Class rdf:about="http://aims.fao.org/aos/agrovoc/c_33359">
        <rdfs:subClassOf rdf:resource="http://www.purl.org/decide#Pathogen"/>
        <rdfs:comment xml:lang="en">Haemophilus somnus classified as Histophilus somni, is a Gram-negative bacterium that can cause respiratory and systemic infections in cattle and other domestic and farm animals</rdfs:comment>
        <rdfs:isDefinedBy xml:lang="en">https://agrovoc.fao.org/browse/agrovoc/en/page/c_33359</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">HistophilusSomnus</rdfs:label>
        <hasExactSynonym xml:lang="en">HS</hasExactSynonym>
    </owl:Class>
    


    <!-- http://aims.fao.org/aos/agrovoc/c_34495 -->

